{
  "alwaysUseActiveTerminal": false,
  "bracketedPaste": false,
  "editor.accessibilitySupport": "off",
  "editor.minimap.enabled": true,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "helpPanel": {
    "cacheIndexFiles": "None",
    "clickCodeExamples": {
      "Click": "Copy",
      "Ctrl+Click": "Run",
      "Shift+Click": "Ignore"
    },
    "enableHoverLinks": true,
    "enableSyntaxHighlighting": true,
    "previewLocalPackages": ["."],
    "rpath": ""
  },
  "libPaths": [],
  "liveShare": {
    "defaults": {
      "commandForward": false,
      "shareBrowser": false,
      "shareWorkspace": true
    },
    "timeout": 10000
  },
  "lsp": {
    "args": [],
    "debug": false,
    "diagnostics": true,
    "enabled": true,
    "lang": "",
    "multiServer": true,
    "promptToInstall": true,
    "use_stdio": false
  },
  "plot": {
    "customStyleOverwrites": "",
    "defaults": {
      "colorTheme": "original",
      "fullWindowMode": false,
      "plotPreviewLayout": "multirow"
    },
    "devArgs": {
      "height": 1200,
      "width": 800
    },
    "timing": {
      "refreshInterval": 10,
      "resizeInterval": 100
    },
    "useHttpgd": false
  },
  "projectManager.git.baseFolders": ["/Users/kam/Documents/Projects/Repos", "/Users/kam/Documents/Projects/Forks"],
  "redhat.telemetry.enabled": false,
  "removeLeadingComments": false,
  "rmarkdown": {
    "chunkBackgroundColor": "rgba(128, 128, 128, 0.1)",
    "codeLensCommands": ["r.runCurrentChunk", "r.runAboveChunks"],
    "enableCodeLens": true,
    "knit": {
      "command": "rmarkdown::render",
      "defaults": {
        "knitWorkingDirectory": "document directory"
      },
      "focusOutputChannel": true,
      "openOutputFile": false,
      "useBackgroundProcess": true
    },
    "preview": {
      "autoRefresh": true,
      "zoom": 1
    }
  },
  "rpath": {
    "linux": "",
    "mac": "",
    "windows": ""
  },
  "rterm": {
    "linux": "",
    "mac": "",
    "option": ["--no-save", "--no-restore"],
    "windows": ""
  },
  "rtermSendDelay": 8,
  "session": {
    "data": {
      "pageSize": 500,
      "rowLimit": 0
    },
    "emulateRStudioAPI": true,
    "levelOfObjectDetail": "Minimal",
    "objectLengthLimit": 2000,
    "objectTimeout": 50,
    "useWebServer": false,
    "viewers": {
      "viewColumn": {
        "browser": "Active",
        "helpPanel": "Two",
        "pageViewer": "Active",
        "plot": "Two",
        "view": "Two",
        "viewer": "Two"
      }
    },
    "watchGlobalEnvironment": true
  },
  "sessionWatcher": true,
  "source": {
    "echo": false,
    "encoding": "UTF-8",
    "focus": "editor"
  },
  "useRenvLibPath": false,
  "workspaceViewer": {
    "clearPrompt": true,
    "removeHiddenItems": false,
    "showObjectSize": false
  },
  "workbench.colorTheme": "Default Dark Modern",
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.linkedEditing": true,
  "jupyter.interactiveWindow.creationMode": "perFile"
}
