{
    "[latex]": {
        "editor.defaultFormatter": "James-Yu.latex-workshop"
    },
    "[markdown]": {
        "editor.defaultFormatter": "DavidAnson.vscode-markdownlint"
    },
    "editor.bracketPairColorization.enabled": true,
    "editor.fontFamily": "hack-nerd-font",
    "editor.fontSize": 14,
    "editor.inlineSuggest.enabled": true,
    "explorer.confirmDelete": false,
    "git.autofetch": true,
    "git.confirmSync": false,
    "jupyter.askForKernelRestart": false,
    "jupyter.widgetScriptSources": [
        "jsdelivr.com",
        "unpkg.com"
    ],
    "latex-workshop.intellisense.file.base": "both",
    "markdown-preview-enhanced.automaticallyShowPreviewOfMarkdownBeingEdited": true,
    "markdown-preview-enhanced.liveUpdate": true,
    "markdown-preview-enhanced.mathRenderingOption": "MathJax",
    "markdown-preview-enhanced.pandocArguments": [
        "--standalone",
        "-F",
        "pandoc-crossref",
        "--citeproc"
    ],
    "markdown-preview-enhanced.usePandocParser": true,
    "projectManager.git.baseFolders": [
        "$home/Documents/Projects/"
    ],
    "redhat.telemetry.enabled": true,
    "sync.gist": "eb606836087b34a186e9627869abcd6a",
    "terminal.external.osxExec": "iterm.app",
    "todo-tree.general.enableFileWatcher": true,
    "todo-tree.general.tags": [
        "NOTE",
        "TODO",
        "FIXME",
        "MARK",
        "[ ]",
        "[x]",
        "HOT"
    ],
    "todo-tree.highlights.customHighlight": {
        "[ ]": {
            "background": "#ff000080",
            "icon": "square",
            "iconColour": "#ff000080"
        },
        "[x]": {
            "background": "#00ff0080",
            "icon": "square-fill",
            "iconColour": "#00ff0080"
        },
        "FIXME": {
            "background": "#FF01FF",
            "icon": "bug",
            "iconColour": "#FF01FF"
        },
        "HOT": {
            "background": "#f80b2180",
            "icon": "flame",
            "iconColour": "#f80b2180"
        },
        "MARK": {
            "background": "#157EFB",
            "icon": "tag",
            "iconColour": "#157EFB"
        },
        "NOTE": {
            "background": "#FFA500",
            "icon": "file",
            "iconColour": "#FFA500"
        },
        "TODO": {
            "background": "#FFD703",
            "icon": "clippy",
            "iconColour": "#FFD703"
        }
    },
    "todo-tree.highlights.defaultHighlight": {
        "fontWeight": "bold",
        "foreground": "#000000",
        "opacity": 90,
        "type": "tag"
    },
    "todo-tree.regex.enableMultiLine": true,
    "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
    "workbench.colorTheme": "Material Theme Ocean High Contrast",
    "workbench.iconTheme": "material-icon-theme",
    "terminal.integrated.fontFamily": "HackNerdFontComplete-Regular"
  }
